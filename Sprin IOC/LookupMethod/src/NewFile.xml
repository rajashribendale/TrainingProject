<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
			"https://www.springframework.org/dtd/spring-beans-2.0.dtd">
			
			<beans>
			<bean id="u" class="com.University" scope="singleton">
			<property name="id" value="11"></property>
			<property name="name" value="shivaji"></property>
			<property name="college" ref="c"></property>
			<lookup-method name="getCollege"/>
			
			
			</bean>
			<bean id="c" class="com.college" scope="prototype">
			<property name="cid" value="12"></property>
			<property name="name" value="PCCOE"></property>
			
			
			</bean>
			
			</beans>